			<% if content_for? :fullpage %>
				<%= yield :fullpage %>
				<% else %>
			  <div class="inner">
					<%= yield %>
			  </div> <!-- /inner -->
			<% end %>
		</div> <!-- /content -->
		<!-- footer
		================================================== -->
		<footer class="site-footer">
		  <div class="copyright">Copyright Â© 2017 Baabao. All Rights Reserved.</div> <!-- /copyright -->
		</footer> <!-- /site-footer -->
	</div> <!-- /wrapper -->
